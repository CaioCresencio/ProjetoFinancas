<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.financasdroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VIEW.DebitosMensais"></activity>
        <activity android:name=".VIEW.CreditosMensais" />
        <activity android:name=".VIEW.OrcamentoFinal" />
        <activity android:name=".VIEW.CriarLancamento" />
        <activity
            android:name=".VIEW.Lancamentos"
            android:label="@string/title_activity_lancamento"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".VIEW.CriarCategoria" />
        <activity
            android:name=".VIEW.Categorias"
            android:label="@string/title_activity_criar_categoria"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VIEW.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>